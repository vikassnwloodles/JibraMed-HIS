        </record>

        <record model="ir.ui.menu-res.group" id="menu_health_group_health_imaging_conf_admin">
            <field name="menu" ref="menu_imaging_test_parent"/>
            <field name="group" ref="group_health_imaging_admin"/>
        </record>


