<form>
    <label name="request"/>
    <field name="request" widget="char"/>
    <label name="date"/>
    <group id="date_and_ugent" colspan="1">
      <field name="date"/>
      <label name="urgent"/>
      <field name="urgent"/>
    </group>
    <label name="source_type"/>
    <group id="lab_sample_source" colspan="1">
      <field name="source_type"/>
      <field name="patient_id"/>
      <field name="other_source"/>
    </group>
    <label name="name"/>
    <field name="name"/>
    <label name="doctor_id"/>
    <field name="doctor_id"/>
    <label name="context"/>
    <field name="context"/>
    <label name="state"/>
    <field name="state" readonly="1"/>
</form>
