<form>
    <notebook>
        <page string="General" id="orthanc_patient_page">
          <group colspan="4" string="Orthanc" id="orthanc_info">
            <label name="name"/>
            <field name="name"/>
            <label name="bd"/>
            <field name="bd"/>
            <label name="ident"/>
            <field name="ident"/>
            <label name="uuid"/>
            <field name="uuid"/>
            <newline/>
            <label name="server"/>
            <field name="server"/>
            <label name="link"/>
            <field name="link" widget="url"/>
          </group>
          <newline/>
          <separator string=" " id="blank_line"/>
          <newline/>
          <group colspan="4" string="GNU Health" id="gnuhealth_info">
            <label name="patient"/>
            <field name="patient" colspan="3"/>
          </group>
        </page>
        <page string="Studies" id="orthanc_patient_studies_page">
            <field name="studies" mode="tree,form"/>
        </page>
    </notebook>
</form>
